umn":44,"offset":312}}}],"position":{"start":{"line":6,"column":1,"offset":269},"end":{"line":6,"column":44,"offset":312}}},{"type":"text","value":"\n"},{"type":"element","tagName":"li","properties":{},"children":[{"type":"text","value":"Administer the organization itself","position":{"start":{"line":7,"column":3,"offset":316},"end":{"line":7,"column":37,"offset":350}}}],"position":{"start":{"line":7,"column":1,"offset":314},"end":{"line":7,"column":37,"offset":350}}},{"type":"text","value":"\n"}],"position":{"start":{"line":3,"column":1,"offset":156},"end":{"line":7,"column":37,"offset":350}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Members are managed on the \"Members\" page of the organization. Only organization administrators can manage members. ","position":{"start":{"line":9,"column":1,"offset":354},"end":{"line":9,"column":117,"offset":470}}}],"position":{"start":{"line":9,"column":1,"offset":354},"end":{"line":9,"column":117,"offset":470}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{},"children":[{"type":"text","value":"Managing Members Manually","position":{"start":{"line":11,"column":4,"offset":477},"end":{"line":11,"column":29,"offset":502}}}],"position":{"start":{"line":11,"column":1,"offset":474},"end":{"line":11,"column":29,"offset":502}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Members are managed manually when synchronization is not available (for Bitbucket Cloud or Azure Devops for example) or when synchronization is deactivated.","position":{"start":{"line":12,"column":1,"offset":504},"end":{"line":12,"column":157,"offset":660}}}],"position":{"start":{"line":12,"column":1,"offset":504},"end":{"line":12,"column":157,"offset":660}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{},"children":[{"type":"text","value":"Adding Members","position":{"start":{"line":14,"column":5,"offset":668},"end":{"line":14,"column":19,"offset":682}}}],"position":{"start":{"line":14,"column":1,"offset":664},"end":{"line":14,"column":19,"offset":682}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"You can add members to an organization using the \"Add a member\" button. Administrators can search manually for SonarCloud users and add them as members.","position":{"start":{"line":15,"column":1,"offset":684},"end":{"line":15,"column":153,"offset":836}}}],"position":{"start":{"line":15,"column":1,"offset":684},"end":{"line":15,"column":153,"offset":836}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{},"children":[{"type":"text","value":"Managing Members in GitHub","position":{"start":{"line":17,"column":4,"offset":843},"end":{"line":17,"column":30,"offset":869}}}],"position":{"start":{"line":17,"column":1,"offset":840},"end":{"line":17,"column":30,"offset":869}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"For organizations that are bound to GitHub, members can be synchronized with GitHub organizations or managed manually. ","position":{"start":{"line":18,"column":1,"offset":871},"end":{"line":18,"column":120,"offset":990}}}],"position":{"start":{"line":18,"column":1,"offset":871},"end":{"line":18,"column":120,"offset":990}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Note that in all cases, members should have a SonarCloud account before being synchronized with GitHub or added manually.","position":{"start":{"line":20,"column":1,"offset":994},"end":{"line":20,"column":122,"offset":1115}}}],"position":{"start":{"line":20,"column":1,"offset":994},"end":{"line":20,"column":122,"offset":1115}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{},"children":[{"type":"text","value":"Synchronizing Members with your GitHub Organization","position":{"start":{"line":22,"column":5,"offset":1123},"end":{"line":22,"column":56,"offset":1174}}}],"position":{"start":{"line":22,"column":1,"offset":1119},"end":{"line":22,"column":56,"offset":1174}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"When ","position":{"start":{"line":23,"column":1,"offset":1176},"end":{"line":23,"column":6,"offset":1181}}},{"type":"element","tagName":"a","properties":{"href":"/SonarQubeChineseDoc8.1/organizations/overview/"},"children":[{"type":"text","value":"importing","position":{"start":{"line":23,"column":7,"offset":1182},"end":{"line":23,"column":16,"offset":1191}}}],"position":{"start":{"line":23,"column":6,"offset":1181},"end":{"line":23,"column":43,"offset":1218}}},{"type":"text","value":" a GitHub organization into SonarCloud, members are automatically synchronized with your GitHub organization.","position":{"start":{"line":23,"column":43,"offset":1218},"end":{"line":23,"column":152,"offset":1327}}},{"type":"element","tagName":"br","properties":{},"children":[],"position":{"start":{"line":23,"column":152,"offset":1327},"end":{"line":24,"column":1,"offset":1331}}},{"type":"text","value":"\n"},{"type":"text","value":"This means that each member of your GitHub organization who has a SonarCloud account will be automatically added to the SonarCloud organization,\nand will have direct access in SonarCloud to the organizations they've been added to. ","position":{"start":{"line":24,"column":1,"offset":1331},"end":{"line":25,"column":87,"offset":1564}}}],"position":{"start":{"line":23,"column":1,"offset":1176},"end":{"line":25,"column":87,"offset":1564}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"You can synchronize a bound organization with manually managed members using the \"Configure synchronization\" button.\nDuring synchronization, members of the SonarCloud organization who are not part of the GitHub organization are removed from the SonarCloud\norganization and members of the GitHub organization who are not members of the SonarCloud organization are added to the SonarCloud organization.","position":{"start":{"line":27,"column":1,"offset":1568},"end":{"line":29,"column":145,"offset":1972}}}],"position":{"start":{"line":27,"column":1,"offset":1568},"end":{"line":29,"column":145,"offset":1972}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"After creating an organization or activating synchronization, SonarCloud users that are added or removed from the GitHub organization are automatically added or removed from\nthe SonarCloud organization. It's not possible to manually add or remove a member when synchronization is activated.","position":{"start":{"line":31,"column":1,"offset":1976},"end":{"line":32,"column":117,"offset":2268}}}],"position":{"start":{"line":31,"column":1,"offset":1976},"end":{"line":32,"column":117,"offset":2268}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Permissions are not synchronized and must be managed manually (see below).","position":{"start":{"line":34,"column":1,"offset":2272},"end":{"line":34,"column":75,"offset":2346}}}],"position":{"start":{"line":34,"column":1,"offset":2272},"end":{"line":34,"column":75,"offset":2346}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h3","properties":{},"children":[{"type":"text","value":"Deactivating Member Synchronization","position":{"start":{"line":36,"column":5,"offset":2354},"end":{"line":36,"column":40,"offset":2389}}}],"position":{"start":{"line":36,"column":1,"offset":2350},"end":{"line":36,"column":40,"offset":2389}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"You can deactivate member synchronization using the \"Configure synchronization\" button.\nWhen you deactivate member synchronization, no members will be added or removed automatically.\nAfter deactivating synchronization, members will be managed manually.","position":{"start":{"line":37,"column":1,"offset":2391},"end":{"line":39,"column":70,"offset":2646}}}],"position":{"start":{"line":37,"column":1,"offset":2391},"end":{"line":39,"column":70,"offset":2646}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{},"children":[{"type":"text","value":"Granting permissions","position":{"start":{"line":42,"column":4,"offset":2655},"end":{"line":42,"column":24,"offset":2675}}}],"position":{"start":{"line":42,"column":1,"offset":2652},"end":{"line":42,"column":24,"offset":2675}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Once users are added or synchronized, organization administrators can grant them permissions to perform specific operations in the organization. It is up to the\nadministrators to make sure each member gets the relevant permissions.","position":{"start":{"line":43,"column":1,"offset":2677},"end":{"line":44,"column":71,"offset":2910}}}],"position":{"start":{"line":43,"column":1,"offset":2677},"end":{"line":44,"column":71,"offset":2910}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"To avoid having to manage individual permissions at a project level, organization admins can create groups to manage permissions\nand add new users to those groups on the \"Members\" page.","position":{"start":{"line":46,"column":1,"offset":2914},"end":{"line":47,"column":57,"offset":3101}}}],"position":{"start":{"line":46,"column":1,"offset":2914},"end":{"line":47,"column":57,"offset":3101}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{},"children":[{"type":"text","value":"Future evolutions","position":{"start":{"line":49,"column":4,"offset":3108},"end":{"line":49,"column":21,"offset":3125}}}],"position":{"start":{"line":49,"column":1,"offset":3105},"end":{"line":49,"column":21,"offset":3125}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value":"Future versions of SonarCloud will make this onboarding process easier for BitBucket Cloud, Azure Devops, and others.\nUsers' permissions will be retrieved from systems and mapped to SonarCloud permissions on a best-effort basis.","position":{"start":{"line":50,"column":1,"offset":3127},"end":{"line":51,"column":111,"offset":3357}}}],"position":{"start":{"line":50,"column":1,"offset":3127},"end":{"line":51,"column":111,"offset":3357}}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":51,"column":111,"offset":3357}}}}